{
  "name": "vmc-api-v3",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "build:local": "bash ./scripts/set_env.bash development && nest build",
    "build:staging": "bash ./scripts/set_env.bash staging && nest build",
    "build:prod": "bash ./scripts/set_env.bash production && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:staging": "yarn build:staging && yarn run:staging",
    "start:prod": "yarn build:prod && yarn run:prod",
    "run:staging": "cross-env NODE_ENV=staging && node dist/src/main",
    "run:prod": "cross-env NODE_ENV=production && node dist/src/main",
    "pretest:int": "yarn test:db:restart",
    "test:db:up": "docker-compose up -d test-db",
    "test:db:clean": "docker exec test-db sh -c \"mongosh --quiet --authenticationDatabase admin --username vmc --password 1234 vmc --eval 'db.dropDatabase()'\"",
    "test:db:data": "docker cp ./test/e2e/configurations/data/users.json test-db:/data/users.json",
    "test:db:prepopulate": "docker exec test-db sh -c \"mongoimport --db=vmc --collection=users --type=json --file=./data/users.json --jsonArray --authenticationDatabase=admin --username=vmc --password=1234\"",
    "test:db:rm": "docker-compose rm -s -f -v test-db",
    "test:db:restart": "yarn test:db:rm && yarn test:db:up",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "jest --config ./test/unit/configurations/unit.config.ts --coverage",
    "test:unit:watch": "yarn test:unit --watch",
    "test:e2e": "jest --config ./test/e2e/configurations/e2e.config.ts --coverage",
    "test:e2e:watch": "yarn test:e2e --watch"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.317.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.2",
    "@nestjs/swagger": "^6.3.0",
    "@sendgrid/mail": "^7.7.0",
    "@typegoose/typegoose": "^10.3.2",
    "@types/passport": "^1.0.12",
    "argon2": "^0.30.3",
    "aws-sdk": "^2.1360.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "chromium": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "clone-buffer": "^1.0.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "facebook-nodejs-business-sdk": "^16.0.2",
    "mongoose": "^7.0.1",
    "nestjs-typegoose": "^7.1.38",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "puppeteer": "^20.8.0",
    "puppeteer-core": "^20.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "stripe": "^12.4.0",
    "which": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.1",
    "@types/escape-string-regexp": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/facebook-nodejs-business-sdk": "^15.0.2",
    "@types/jest": "29.2.4",
    "@types/node": "^20.4.5",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  }
}
